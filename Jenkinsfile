           }
        }
      stage ("test stage"){
         steps {
           echo "testing"
           }
        }
      stage ("archive stage"){
         steps {
           echo "archiving"
           }
        }
      stage ("deploy stage"){
         steps {
           echo "deploying"
           }
        }
    }	

}
